<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Design.PreviewWith>
    <Border Padding="20">
      <StackPanel>
        <CheckBox Content="Box"/>
        <Button Content="Button Text"></Button>
      </StackPanel>
    </Border>
  </Design.PreviewWith>
  
  <!-- Window Styles -->
  <Style Selector="Window">
    <Setter Property="SizeToContent" Value="Height" />
    <Setter Property="WindowStartupLocation" Value="CenterOwner" />
    <Setter Property="TransparencyLevelHint" Value="AcrylicBlur" />
    <Setter Property="Background">
      <Setter.Value>
        <LinearGradientBrush StartPoint="0%,0%" EndPoint="100%,100%">
          <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0" Color="#0F1419"/>
            <GradientStop Offset="0.3" Color="#1A1F2E"/>
            <GradientStop Offset="0.7" Color="#1E2536"/>
            <GradientStop Offset="1" Color="#242B3D"/>
          </LinearGradientBrush.GradientStops>
        </LinearGradientBrush>
      </Setter.Value>
    </Setter>
    <Setter Property="ExtendClientAreaToDecorationsHint" Value="True" />
    <Setter Property="Icon" Value="/Assets/netw1re-logo.ico" />
    <Setter Property="FontFamily" Value="/Assets/Fonts/Montserrat.ttf#Montserrat" />
    <Setter Property="FontWeight" Value="Normal" />
    <Setter Property="Foreground" Value="#FFFFFF" />
  </Style>
  
  <!-- Enhanced Typography Styles -->
  <Style Selector="TextBlock.h1">
    <Setter Property="FontWeight" Value="Bold"/>
    <Setter Property="FontSize" Value="24"/>
    <Setter Property="Foreground" Value="#FFFFFF"/>
    <Setter Property="Margin" Value="0,0,0,16"/>
    <Setter Property="LetterSpacing" Value="0.5"/>
    <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
  </Style>
  
  <Style Selector="TextBlock.h2">
    <Setter Property="FontWeight" Value="SemiBold"/>
    <Setter Property="FontSize" Value="20"/>
    <Setter Property="Foreground" Value="#E8F4FD"/>
    <Setter Property="Margin" Value="0,0,0,12"/>
    <Setter Property="LetterSpacing" Value="0.3"/>
  </Style>
  
  <Style Selector="TextBlock.h3">
    <Setter Property="FontWeight" Value="SemiBold"/>
    <Setter Property="FontSize" Value="16"/>
    <Setter Property="Foreground" Value="#E8F4FD"/>
    <Setter Property="Margin" Value="0,0,0,8"/>
  </Style>
  
  <Style Selector="TextBlock.p">
    <Setter Property="TextWrapping" Value="Wrap"/>
